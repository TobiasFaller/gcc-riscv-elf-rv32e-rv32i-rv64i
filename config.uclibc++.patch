 HAVE_DOT_CONFIG=y
@@ -17,12 +17,12 @@
 #
 # UCLIBCXX_HAS_WCHAR is not set
 UCLIBCXX_IOSTREAM_BUFSIZE=32
-UCLIBCXX_HAS_LFS=y
+UCLIBCXX_HAS_LFS=n
 UCLIBCXX_SUPPORT_CDIR=y
 UCLIBCXX_SUPPORT_CIN=y
 UCLIBCXX_SUPPORT_COUT=y
 UCLIBCXX_SUPPORT_CERR=y
-# UCLIBCXX_SUPPORT_CLOG is not set
+UCLIBCXX_SUPPORT_CLOG=y
 
 #
 # STL and Code Expansion
@@ -42,13 +42,13 @@
 #
 # Library Installation Options
 #
-UCLIBCXX_RUNTIME_PREFIX="/usr/uClibc++"
+UCLIBCXX_RUNTIME_PREFIX="/usr/local/riscv32e"
 UCLIBCXX_RUNTIME_INCLUDE_SUBDIR="/include"
 UCLIBCXX_RUNTIME_LIB_SUBDIR="/lib"
 UCLIBCXX_RUNTIME_BIN_SUBDIR="/bin"
-UCLIBCXX_EXCEPTION_SUPPORT=y
-IMPORT_LIBSUP=y
-IMPORT_LIBGCC_EH=y
+UCLIBCXX_EXCEPTION_SUPPORT=n
+IMPORT_LIBSUP=n
+IMPORT_LIBGCC_EH=n
 BUILD_STATIC_LIB=y
-# BUILD_ONLY_STATIC_LIB is not set
+BUILD_ONLY_STATIC_LIB=y
 # DODEBUG is not set